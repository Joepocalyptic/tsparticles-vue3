{
  "name": "nuxt3-particles-demo",
  "version": "2.12.0",
  "private": true,
  "scripts": {
    "setup-vue-demi": "pnpm exec vue-demi-switch 3",
    "build": "pnpm run setup-vue-demi && nuxt build",
    "dev": "pnpm run setup-vue-demi && nuxt dev",
    "generate": "pnpm run setup-vue-demi && nuxt generate",
    "preview": "pnpm run setup-vue-demi && nuxt preview",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "pnpm run lint:js && pnpm run lint:prettier",
    "lintfix": "prettier --write --list-different . && pnpm run lint:js --fix"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --cache",
    "*.**": "prettier --check --ignore-unknown"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@types/node": "^18.17.3",
    "nuxt": "^3.6.5"
  },
  "dependencies": {
    "tsparticles": "^3.0.0-beta.1",
    "tsparticles-demo-configs": "^2.12.0",
    "@tsparticles/vue": "workspace:^"
  }
}
